@{
    Layout = "~/Views/Shared/AdminLTE/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Admin</title>
</head>
<body>
    <div class="container-fluid">
        <div class="row">
            <div class="col-5">
                <div class="card" id="PersonContainer">
                    <div class="card-header bg-transparent border-success">Gender Chart</div>
                    <div class="card-body">
                        <div id="PersonCh"></div>
                    </div>
                </div>
            </div>
            <div class="col-5">
                <div class="card" id="UniversityContainer">
                    <div class="card-header bg-transparent border-success">University Chart</div>
                    <div class="card-body">
                        <div id="UnivCh"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-7">
                <div class="card" id="SalaryContainer">
                    <div class="card-header bg-transparent border-success">Salary Chart</div>
                    <div class="card-body">
                        <div id="SalaryCh"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
@section scripts{
    <script src="~/js/Admin.js" asp-append-version="true"></script>
}